{
    "name": "@atlauncher/discord-bot",
    "version": "1.0.0",
    "description": "This is the code for our Discord bot which runs on our official Discord server at https://atl.pw/discord",
    "main": "dist",
    "scripts": {
        "build": "tsc",
        "build:watch": "npm run build -- -w",
        "clean": "atlauncher-scripts clean",
        "config:base64": "ts-node utils/base64Config.ts",
        "dev": "cross-env NODE_ENV=development nodemon -w src -e ts --exec \"npm run build && npm run start\"",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "lint:watch": "npm run lint -- --watch",
        "oauth:url": "ts-node utils/getOAuthUrl.ts",
        "prepush": "npm run lint",
        "start": "node dist/app.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ATLauncher/Discord-Bot.git"
    },
    "keywords": [
        "discord",
        "atlauncher",
        "bot"
    ],
    "author": "Ryan Dowling <ryan@ryandowling.me>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ATLauncher/Discord-Bot/issues"
    },
    "homepage": "https://github.com/ATLauncher/Discord-Bot#readme",
    "dependencies": {
        "@atlauncher/atlauncher-scripts": "0.1.0-15",
        "sequelize": "^4.11.5",
        "sqlite3": "^3.1.13",
        "yamdbf": "^3.0.1"
    },
    "devDependencies": {
        "@types/node": "^8.0.31",
        "@types/sequelize": "^4.0.75",
        "@types/sqlite3": "^3.1.1",
        "cross-env": "^5.0.5",
        "husky": "^0.14.3",
        "nodemon": "^1.12.1",
        "ts-node": "~3.3.0",
        "tslint": "~5.7.0",
        "typescript": "~2.5.3"
    },
    "engineStrict": true,
    "engines": {
        "node": ">= 8",
        "npm": ">=5"
    }
}
